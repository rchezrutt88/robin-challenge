{
  "name": "robin-challenge",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon ./dist/index.js",
    "dev": "nodemon -e ts,js,json --inspect=0.0.0.0 -w ./src --exec \"tsc --incremental && node ./dist/app.js\"",
    "test": "jest"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jest": "^25.1.0",
    "lodash": "^4.17.15",
    "luxon": "^1.22.0",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.2",
    "@types/http-errors": "^1.6.3",
    "@types/jest": "^25.1.2",
    "@types/lodash": "^4.14.149",
    "@types/luxon": "^1.21.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
